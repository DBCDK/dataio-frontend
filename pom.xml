                    <nohelp>true</nohelp>
                    <excludePackageNames>dk.dbc.oss.ns.*:info.lc.xmlns.*</excludePackageNames>
                    <tags>
                        <tag>
                            <name>todo</name>
                        </tag>
                    </tags>
                    <sourceFileExcludes>
                        <sourceFileExclude>${project.build.directory}/generated-sources/**/*.java</sourceFileExclude>
                    </sourceFileExcludes>
                </configuration>
            </plugin>

        </plugins>
    </reporting>

    <pluginRepositories>
